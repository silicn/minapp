/* pages/components/freelabel/freeLabel.wxss */

.container{
    display: flex;
    background: linear-gradient(to right,#FF5A4B,#ffffff);
    border-radius: 9999px;
    justify-content: flex-end;
    align-items: center;
    padding: 8rpx;
}
.title{
    color: #FF5A4B;
    font-size: 13px;
    margin-right: 30rpx;
}
.circle{
    display: inline-block;
    color: white;
    font-size: 15px;
    background: #FF5A4B;
    margin-right: 10rpx;
    border-radius: 50%;
    padding: 6rpx;
}